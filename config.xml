<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jtdigi.cidmeexplorer" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CIDMEExplorer</name>
    <description>
        A proof-of-concept app demonstrating a User Interface (UI) for visual CIDME Entity manipulation.
    </description>
    <author email="joe@joethielen.com">
        Joe Thielen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/cidme-icon_logo-48x48.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon background="www/img/cidme-icon_logo_background-432x432.png" foreground="www/img/cidme-icon_logo_foreground-432x432.png" src="www/img/cidme-icon_logo-48x48.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="https://github.com/apache/cordova-plugin-whitelist" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-sqlite-storage" spec="^5.0.1" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.4" />
</widget>
