<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jtdigi.cidmeexplorer" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CIDMEExplorer</name>
    <description>
        A proof-of-concept app demonstrating a User Interface (UI) for visual CIDME Entity manipulation.
    </description>
    <author email="joe@joethielen.com">
        Joe Thielen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/cidme-icon_logo-48x48.png" />
    <allow-navigation href="*" />
</widget>
